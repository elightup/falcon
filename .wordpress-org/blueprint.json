{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"preferredVersions": {
		"php": "8.3",
		"wp": "6.4"
	},
	"features": {
		"networking": true
	},
	"phpExtensionBundles": [
		"kitchen-sink"
	],
	"landingPage": "\/wp-admin\/admin.php?page=falcon",
	"steps": [
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "wordpress.org\/plugins",
				"slug": "falcon"
			},
			"options": {
				"activate": true
			}
		},
		{
			"step": "setSiteOptions",
			"options": {
				"blogname": "Falcon Demo"
			}
		},
		{
			"step": "login",
			"username": "admin",
			"password": "password"
		}
	]
}